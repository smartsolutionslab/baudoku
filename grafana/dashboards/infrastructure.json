  "panels": [
    {
      "title": "PostgreSQL Active Connections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "db_client_connections_usage{job=~\"baudoku.*\", state=\"used\"}",
          "legendFormat": "{{job}} used"
        },
        {
          "expr": "db_client_connections_usage{job=~\"baudoku.*\", state=\"idle\"}",
          "legendFormat": "{{job}} idle"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2, "stacking": { "mode": "normal" } }
        }
      }
    },
    {
      "title": "DB Connection Pool Wait Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(db_client_connections_wait_time_seconds_sum{job=~\"baudoku.*\"}[5m])",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        }
      }
    },
    {
      "title": "Redis Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "Used Memory"
        },
        {
          "expr": "redis_memory_max_bytes",
          "legendFormat": "Max Memory"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        }
      }
    },
    {
      "title": "Redis Connected Clients",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Clients"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "RabbitMQ Queue Depth",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rabbitmq_queue_messages{queue=~\"baudoku.*\"}",
          "legendFormat": "{{queue}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2 }
        }
      }
    },
    {
      "title": "RabbitMQ Message Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(rabbitmq_queue_messages_published_total{queue=~\"baudoku.*\"}[5m])",
          "legendFormat": "{{queue}} published"
        },
        {
          "expr": "rate(rabbitmq_queue_messages_delivered_total{queue=~\"baudoku.*\"}[5m])",
          "legendFormat": "{{queue}} delivered"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
        }
      }
    },
    {
      "title": ".NET Runtime GC Collections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(process_runtime_dotnet_gc_collections_count_total{job=~\"baudoku.*\"}[5m])",
          "legendFormat": "{{job}} gen{{generation}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 }
